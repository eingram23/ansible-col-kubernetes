apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: traefik{{ suffix }}-local-lan
  namespace: kube-system
spec:
  secretName: traefik{{ suffix }}-local-lan-tls
  issuerRef:
    kind: ClusterIssuer
    name: vault-cluster-issuer
  commonName: traefik{{ suffix }}.local.lan
  dnsNames:
  - traefik{{ suffix }}.local.lan
