apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: calibre-web-tls-acme
  namespace: calibre
spec:
  secretName: calibre-web-tls
  issuerRef:
    kind: ClusterIssuer
    name: acme-cluster-issuer
  commonName: calibre-web{{ suffix }}.ycdisp.net
  dnsNames:
  - calibre-web{{ suffix }}.ycdisp.net
